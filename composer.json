{
    "name": "landmarx/sandbox",
    "license": "MIT",
    "type": "project",
    "description": "Geolocatable node mapping system sandboxed application.",
    "authors": [
        {
            "name": "David Durost",
            "homepage": "http://daviddurost.net"
        }
    ],
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "jquery/jquery",
                "version": "2.1.1",
                "dist": {
                    "url": "https://code.jquery.com/jquery-2.1.1.min.js",
                    "type": "file"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "landmarx/bootstrap-vertical-tabs",
                "version": "1.0.0",
                "dist": {
                    "url": "https://github.com/dbtek/bootstrap-vertical-tabs/archive/1.1.0.zip",
                    "type": "zip"
                }
            }
        }
    ],
    "require": {
        "php":                                  ">=5.4",

        "symfony/symfony":                      "~2.4",
        "symfony/assetic-bundle":               "~2.3",
        "symfony/swiftmailer-bundle":           "~2.3",
        "symfony/monolog-bundle":               "~2.4",
        "symfony/translation":                  "*",
        "symfony-cmf/routing-bundle":           "1.3.*@dev",
        
        "doctrine/mongodb":                     "1.0.*@dev",
        "doctrine/mongodb-odm":                 "1.0.*@dev",
        "doctrine/mongodb-odm-bundle":          "3.0.*@dev",
        "doctrine/data-fixtures":               "1.0.x-dev",
        "doctrine/doctrine-fixtures-bundle":    "2.2.*",
        
        "iampersistent/mongodb-acl-bundle":     "dev-master",
        "twig/extensions":                      "1.0.1",
        "incenteev/composer-parameter-handler": "~2.0",
        
        "sensio/distribution-bundle":           "~2.3",
        "sensio/framework-extra-bundle":        "~3.0",
        "sensio/generator-bundle":              "~2.3",
        
        "jms/aop-bundle":                       "1.0.1",
        "jms/security-extra-bundle":            "1.5.*@dev",
        "jms/di-extra-bundle":                  "1.4.*@dev",
        "jms/translation-bundle":               "1.1.*",
        "jms/serializer-bundle":                "*",
        
        "gedmo/doctrine-extensions":            "2.3.x-dev",
        "stof/doctrine-extensions-bundle":      "~1.1@dev",
        
        "friendsofsymfony/user-bundle":         "2.0.*@dev",
        "friendsofsymfony/elastica-bundle":     "3.0.5",
        "friendsofsymfony/rest-bundle":         "~1.0",
        
        "whiteoctober/breadcrumbs-bundle":      "dev-master",
        "white-october/pagerfanta-bundle":      "dev-master",
        "pagerfanta/pagerfanta":                "dev-master",
        
        "braincrafted/bootstrap-bundle":        "dev-master",
        "twitter/bootstrap":                    "2.3.*",
        "jquery/jquery":                        "2.1.1",
        "leafo/lessphp":                        "dev-master",
        "kriswallsmith/buzz":                   "dev-master",
        
        "willdurand/geocoder":                  "@stable",
        "egeloen/google-map-bundle":            "1.0",
        "oh/google-map-form-type-bundle":       "*",
        
        "vich/uploader-bundle":                 "dev-master",
        
        "ner0tic/php-api-core":                 "2.0.*",
        "ner0tic/php-geopoint-api":             "*",
        "ner0tic/foursquare-api":               "2.0.*",
        
        "vich/uploader-bundle":                 "dev-master",
        "knplabs/knp-gaufrette-bundle":         "dev-master",
        
        "knplabs/knp-menu":                     "2.0.x-dev",
        "knplabs/knp-menu-bundle":              "dev-master",
        
        "liip/imagine-bundle":                  "dev-master",
        "imagine/imagine":                      "*",
        
        "genemu/form-bundle":                   "2.2.*",
        "landmarx/bootstrap-vertical-tabs":     "1.0.0",
        
        "sylius/resource-bundle":               "dev-master",
        "sylius/resource":                      "dev-master"
    },
    "require-dev": {
        "elao/web-profiler-extra-bundle":       "dev-master",
        "jms/debugging-bundle":                 "dev-master",
        "doctrine/doctrine-fixtures-bundle":    "2.2.*",
        "fzaninotto/faker":                     "1.5.*@dev",
        "phpspec/phpspec":                      "~2.0",
        "phpunit/phpunit":                      "~4.1.0"
    },
    "config": {
        "bin-dir": "bin",
        "process-timeout": 20000
    },
    "minimum-stability": "dev",
    "extra": {
        "symfony-app-dir": "app",
        "symfony-web-dir": "web",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        }
    },
    "autoload": {
        "psr-0": {
            "Landmarx": "src/"
        }
    }
}
